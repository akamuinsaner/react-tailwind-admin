{
  "name": "react-tailwind-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prebuild": "rimraf dist",
    "build:server": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:server": "nest start",
    "start:server:dev": "nest start --watch",
    "start:server:debug": "nest start --debug --watch",
    "start:server:prod": "node dist/main",
    "lint:server": "eslint \"server/{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@heroicons/react": "^2.1.1",
    "@nestjs/common": "^10.3.5",
    "@nestjs/core": "^10.3.5",
    "@nestjs/platform-express": "^10.3.5",
    "chart.js": "^4.4.2",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.10",
    "deepmerge": "^4.3.1",
    "dotenv": "^16.4.5",
    "framer-motion": "^11.0.20",
    "next": "14.1.3",
    "randomcolor": "^0.6.2",
    "react": "^18",
    "react-dom": "^18",
    "react-viewer": "^3.2.2",
    "recharts": "^2.12.3",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "source-map-support": "^0.5.21",
    "tailwind-children": "^0.5.0",
    "tailwind-merge": "^2.2.1",
    "unsplash-js": "^7.0.19",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@mertasan/tailwindcss-variables": "^2.7.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "postcss": "^8",
    "prettier": "3.2.5",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.3.0",
    "tw-colors": "^3.3.1",
    "typescript": "^5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "server/src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
